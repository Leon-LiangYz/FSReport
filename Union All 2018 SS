select "Adjusted Year", "Adjusted Quarter", "Adjusted Month", "Order Date","Partially Categorized Product", "Categorized Product","Product Line","License Type","Edition","Major Language","Category","Major Category","Order Status",  "Bill to Country", "Bill to Territory", "Operator Center",sum("Converted Price")
from T2018Q1SS
where ("Operator Center" not like "%United States%")
group by "Adjusted Year", "Adjusted Quarter", "Adjusted Month", "Order Date","Partially Categorized Product", "Categorized Product","Product Line","License Type","Edition","Major Language","Category","Major Category","Order Status",  "Bill to Country", "Bill to Territory","Operator Center"

Union ALL

select "Adjusted Year", "Adjusted Quarter", "Adjusted Month", "Order Date","Partially Categorized Product", "Categorized Product","Product Line","License Type","Edition","Major Language","Category","Major Category","Order Status",  "Bill to Country", "Bill to Territory", "Operator Center",sum("Converted Price")
from T2018AprilSS
where ("Operator Center" not like "%United States%")
group by "Adjusted Year", "Adjusted Quarter", "Adjusted Month", "Order Date","Partially Categorized Product", "Categorized Product","Product Line","License Type","Edition","Major Language","Category","Major Category","Order Status",  "Bill to Country", "Bill to Territory","Operator Center"

Union all

