select "year", "quarter", "month", "Posting_Date","Partially Categorized Product", "Categorized Product","Product Line","License Type","Edition", "Major Language", "Category","Major Category","Cancelled","Bill_to_Country_Region_Code","Bill to Territory", "Operator Center",sum("converted amount")
from T2018DynQ1
group by "year", "quarter", "month", "Posting_Date","Partially Categorized Product", "Categorized Product","Product Line","License Type","Edition", "Major Language", "Category","Major Category","Cancelled","Bill_to_Country_Region_Code","Bill to Territory", "Operator Center"


Union all

